{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "api/orders.js": {
      "maxDuration": 10
    },
    "api/admin-auth.js": {
      "maxDuration": 10
    },
    "api/auth.js": {
      "maxDuration": 10
    },
    "api/users.js": {
      "maxDuration": 10
    },
    "api/payment-redirect.js": {
      "maxDuration": 10
    },
    "api/payapp-proxy.js": {
      "maxDuration": 10
    },
    "api/check-env.js": {
      "maxDuration": 10
    },
    "api/payapp-feedback.js": {
      "maxDuration": 10
    },
    "api/payapp-payments.js": {
      "maxDuration": 10
    },
    "api/packages.js": {
      "maxDuration": 10
    },
    "api/upload-image.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/admin/index.html"
    },
    {
      "source": "/myinfo",
      "destination": "/pages/my-info.html"
    },
    {
      "source": "/myinfo.html",
      "destination": "/pages/my-info.html"
    }
  ],
  "headers": [
    {
      "source": "/pages/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    },
    {
      "source": "/api/payapp-feedback",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        }
      ]
    },
    {
      "source": "/api/payapp-payments",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        },
        {
          "key": "Access-Control-Allow-Methods",
          "value": "GET, POST, OPTIONS"
        }
      ]
    }
  ],
  "public": true
}