{
  "buildCommand": "echo 'No build required'",
  "outputDirectory": ".",
  "framework": null,
  "functions": {
    "api/orders.js": {
      "maxDuration": 10
    },
    "api/admin-auth.js": {
      "maxDuration": 10
    },
    "api/auth.js": {
      "maxDuration": 10
    },
    "api/users.js": {
      "maxDuration": 10
    },
    "api/payment-redirect.js": {
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/",
      "destination": "/index.html"
    },
    {
      "source": "/admin",
      "destination": "/admin/index.html"
    },
    {
      "source": "/admin/",
      "destination": "/admin/index.html"
    },
    {
      "source": "/pages/payment-complete",
      "destination": "/pages/payment-complete.html"
    },
    {
      "source": "/pages/checkout",
      "destination": "/pages/checkout.html"
    },
    {
      "source": "/pages/register",
      "destination": "/pages/register.html"
    }
  ],
  "headers": [
    {
      "source": "/pages/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ]
}